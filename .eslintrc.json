{
    "env": {
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@babel/eslint-parser",
    "rules": {
        "class-methods-use-this": "off",
        "no-restricted-syntax": "off",
        "space-before-blocks": "warn",
        "linebreak-style": 0,
        "max-len": [
            "warn",
            160
        ],
        "quotes": [
            "error",
            "single"
        ]
    }
}